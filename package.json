{
  "name": "puppeteer-cli-template",
  "version": "0.0.1",
  "description": "",
  "bin": {
    "puppeteer-cli-template": "./dist/src/cli.js"
  },
  "scripts": {
    "test": "nyc mocha",
    "doc": "typedoc --out ./docs -m \"amd\"",
    "lint": "pretty-quick && tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' --fix",
    "build": "tsc",
    "watch": "tsc -w",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "@types/puppeteer": "^1.5.1",
    "chai": "^4.1.0",
    "espower-typescript": "^8.0.2",
    "isparta": "^4.0.0",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "power-assert": "^1.4.4",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.7.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.14.0",
    "typedoc": "^0.10.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "puppeteer": "^1.6.1"
  }
}
